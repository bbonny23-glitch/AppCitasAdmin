{
  "name": "Flutter Codespace",
  "build": { "dockerfile": "Dockerfile" },
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "forwardPorts": [8081],
  "extensions": ["Dart-Code.flutter"],
  "postCreateCommand": "flutter config --enable-web && flutter pub get",
  "remoteUser": "root"
}